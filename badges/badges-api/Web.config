    <add key="AWSProfileName" value="default" />
    <add key="AWSRegion" value="ap-southeast-2" />
    <add key="PAPERTRAIL_SYSTEM" value="phl-badges" />
    <add key="PAPERTRAIL_PROGRAM_API" value="badges-api" />
    <add key="BambooHR" value="https://api.bamboohr.com/api/gateway.php/propellerhead/v1/employees/directory" />
    <add key="BambooHRKey" value="YTRjNWFiYTQ0MWU4MjJiNWY5YWMwMTk4Mzc1MjhmZWE2ZmMzZGUyYzp6" />
  </appSettings>

  <log4net>
    <appender name="PapertrailRemoteSyslogAppender" type="log4net.Appender.RemoteSyslogAppender">
      <facility value="Local6" />
      <identity value="%date{yyyy-MM-ddTHH:mm:ss.ffffffzzz} %property{log4net:HostName} %property{log4net:Program}" />
      <layout type="log4net.Layout.PatternLayout" value="%level - %message%newline%exception" />
      <!--<remoteAddress value="%property{log4net:Host}" />-->
      <!--remoteAddress value="logs6.papertrailapp.com" />
      <remotePort value="50129" /-->

      <remoteAddress value="logs5.papertrailapp.com" />
      <remotePort value="12291" />
      <!--<remotePort value="%property{log4net:Port}" />-->
    </appender>
    <root>
      <level value="DEBUG" />
      <appender-ref ref="PapertrailRemoteSyslogAppender" />
    </root>
  </log4net>
  <system.web>
    <compilation debug="true" targetFramework="4.6.1" />
    <httpRuntime targetFramework="4.6.1" />
  </system.web>
  <system.webServer>
    
    <validation validateIntegratedModeConfiguration="false" />
